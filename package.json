{
  "name": "dds-viewer",
  "displayName": "DDS Image Viewer",
  "description": "A VSCode extension for viewing DirectDraw Surface (DDS) texture files with export capabilities",
  "version": "1.0.0",
  "publisher": "YourPublisherName",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Visualization",
    "Other"
  ],
  "keywords": [
    "dds",
    "texture",
    "image",
    "viewer",
    "directx",
    "gpu",
    "compression"
  ],
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/dds-viewer"
  },
  "bugs": {
    "url": "https://github.com/yourusername/dds-viewer/issues"
  },
  "homepage": "https://github.com/yourusername/dds-viewer#readme",
  "main": "./extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "ddsViewer.ddsViewer",
        "displayName": "DDS Viewer",
        "selector": [
          {
            "filenamePattern": "*.dds"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "activationEvents": [
    "onCustomEditor:ddsViewer.ddsViewer",
    "onCommand:ddsViewer.exportAsPNG"
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/vscode": "^1.60.0"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "echo No compilation needed for JavaScript",
    "watch": "echo No watch script configured",
    "pretest": "npm run compile",
    "test": "echo No tests configured",
    "lint": "echo No linter configured"
  },
  "extensionDependencies": []
}
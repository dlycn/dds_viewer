{
  "name": "dds-viewer",
  "displayName": "DDS Viewer",
  "description": "A VSCode extension for previewing DDS image files",
  "version": "0.0.2",
  "license": "MIT",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Visualization",
    "Other"
  ],
  "main": "./extension.js",
  "icon": "./icon.png",
  "contributes": {
    "customEditors": [
      {
        "viewType": "dds-viewer.preview",
        "displayName": "DDS Preview",
        "selector": [
          {
            "filenamePattern": "*.dds"
          }
        ]
      }
    ],
    "commands": [
      {
        "command": "dds-viewer.previewDDS",
        "title": "Preview DDS File",
        "category": "DDS Viewer"
      }
    ],
    "menus": {
      "command-palette": [
        {
          "command": "dds-viewer.previewDDS"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/vscode": "^1.105.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "eslint": "^9.36.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  },
  "repository": {
    "type": "git",
    "url": "https://gitcode.com/StarsAll_InFrost/dds-viewer.git"
  }
}